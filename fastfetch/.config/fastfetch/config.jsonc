{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/ascii.txt"
  },
  "display": {
    "color": {
      "title": "#eb6f92",
      "keys": "#9ccfd8",
      "output":"#e0def4",
      "separator": "#31748f"
    },
    "separator": " â€º "
  },
  "modules": [
    "title",
    "separator",
    "os",
    "kernel",
    "shell",
    {
      "type": "terminal",
      "key": "Terminal",
      "format": "{process-name}"
    },
    "cpu",
    {
      "type": "gpu",
      "key": "GPU",
      "format": "{name} {dedicated-total}"
    },
    {
      "type": "memory",
      "key": "Memory",
      "format": "{used} / {total}"
    },
    // {
    //   "type": "disk",
    //   "key": "Disk",
    //   "folders": "/mnt/c",
    //   "format": "{size-used} / {size-total}"
    // },
    {
      "type": "colors",
      "symbol": "circle",
    }
  ]
}
